<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.henrik.googlemapsexample.Activity_FilterMenu"
    android:background="#ffffff">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/scrollView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:text="@string/kind_of_restaurant"
                android:id="@+id/textView"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"
                android:textAlignment="center" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Price:"
                android:id="@+id/textView2"
                android:textColor="#000000"
                android:textStyle="bold"
                android:textSize="14dp"
                android:layout_centerHorizontal="true"
                android:layout_below="@+id/textView"
                android:layout_marginTop="20dp" />

            <SeekBar
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:id="@+id/seekBar"
                android:layout_below="@+id/textView2"
                android:layout_marginTop="10dp"
                android:progress="50" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton"
                android:layout_below="@+id/seekBar"
                android:layout_marginTop="10dp" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton2"
                android:layout_below="@+id/seekBar"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="10dp" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton3"
                android:layout_alignTop="@+id/toggleButton2"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton4"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/toggleButton" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton5"
                android:layout_below="@+id/toggleButton2"
                android:layout_alignLeft="@+id/toggleButton2"
                android:layout_alignStart="@+id/toggleButton2" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton6"
                android:layout_below="@+id/toggleButton3"
                android:layout_alignLeft="@+id/toggleButton3"
                android:layout_alignStart="@+id/toggleButton3" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton7"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/textView3" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton8"
                android:layout_alignLeft="@+id/toggleButton5"
                android:layout_alignStart="@+id/toggleButton5"
                android:layout_below="@+id/textView3" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton9"
                android:layout_alignLeft="@+id/toggleButton6"
                android:layout_alignStart="@+id/toggleButton6"
                android:layout_below="@+id/textView3" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton10"
                android:layout_below="@+id/toggleButton7" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton11"
                android:layout_below="@+id/toggleButton8"
                android:layout_alignLeft="@+id/toggleButton8" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kind of food:"
                android:id="@+id/textView3"
                android:layout_below="@+id/toggleButton5"
                android:textColor="#000000"
                android:textSize="20dp"
                android:textStyle="bold"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton12"
                android:layout_below="@+id/toggleButton9"
                android:layout_alignParentRight="true" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton13"
                android:layout_below="@+id/toggleButton10"
                android:layout_alignParentLeft="true" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton14"
                android:layout_below="@+id/toggleButton11"
                android:layout_alignLeft="@+id/toggleButton11" />

            <ToggleButton
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:text="New ToggleButton"
                android:id="@+id/toggleButton15"
                android:layout_below="@+id/toggleButton12"
                android:layout_alignParentRight="true" />
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>
